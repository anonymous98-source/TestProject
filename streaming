1Ô∏è‚É£ Enterprise Use Case ‚Äì Fully Connected + Alternate + Admin

üìÑ BC_OR_Enterprise_UseCase.xml

<mxfile host="app.diagrams.net">
  <diagram name="Enterprise_UseCase">
    <mxGraphModel grid="1" gridSize="10">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- System Boundary -->
        <mxCell id="sys" value="BC &amp; OR Portal System" style="rounded=1;strokeWidth=2;" vertex="1" parent="1">
