MuiMenu: {
  styleOverrides: {
    paper: ({ theme }) => ({
      backdropFilter: 'none',
      backgroundColor:
        theme.palette.mode === 'dark'
          ? theme.palette.background.paper
          : '#fff',
      boxShadow: theme.shadows[8],
      borderRadius: theme.shape.borderRadius,
    }),
  },
}, 


MuiPopover: {
  styleOverrides: {
    paper: ({ theme }) => ({
      backdropFilter: 'none',
      backgroundColor:
        theme.palette.mode === 'dark'
          ? theme.palette.background.paper
          : '#fff',
    }),
  },
},
